<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<outlierDetection>
	<pathToData>../mdcstream-wrapper/dataRoot/</pathToData>
	<pathToResult>../mdcstream-wrapper/results/</pathToResult>

	<algorithms>

		<algorithm id="MCOD">
			<k>10</k>
			<radius>0.5</radius>
			<windowSize>1000</windowSize>
		</algorithm>

		<algorithm id="simpleCod">
			<k>10</k>
			<radius>0.5</radius>
			<windowSize>1000</windowSize>
		</algorithm>

		<algorithm id="abstractC">
			<k>10</k>
			<radius>0.5</radius>
			<windowSize>1000</windowSize>
		</algorithm>
		
		<algorithm id="exactSTORM">
			<k>10</k>
			<radius>0.5</radius>
			<windowSize>1000</windowSize>
			<queryFreq>100</queryFreq>
		</algorithm>
		
		<algorithm id="approxSTORM">
			<k>10</k>
			<radius>0.5</radius>
			<windowSize>1000</windowSize>
			<queryFreq>100</queryFreq>
			<p>0.5</p>
		</algorithm>
	</algorithms>

</outlierDetection>



